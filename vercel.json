{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/r/(.*)", "dest": "/api/r/$1" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
  